    "inert": "~3.2",
    "isomorphic-fetch": "~2.2",
    "joi": "~7.3",
    "json-loader": "~0.5",
    "json5": "~0.4",
    "lodash": "~4.3",
    "mocha": "~2.4",
    "postcss-loader": "~0.8",
    "precss": "~1.4",
    "pretty-error": "~2.0",
    "react": "~0.14",
    "react-addons-test-utils": "~0.14",
    "react-document-meta": "~2.0",
    "react-dom": "~0.14",
    "react-redux": "~4.4",
    "react-router": "~2.0",
