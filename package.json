{
  "name": "rook",
  "description": "Opinionated React/Redux/Hapi universal framework",
  "author": "Andre Azzolini <apazzolini@gmail.com> (http://github.com/apazzolini)",
  "license": "MIT",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apazzolini/rook.git"
  },
  "homepage": "https://github.com/apazzolini/rook",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "renderer",
    "boilerplate",
    "babel",
    "npm",
    "hapi"
  ],
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --presets es2015,stage-0,react --plugins transform-runtime src/ --out-dir lib/",
    "dev": "./dev-scripts/copy-to-app.sh",
    "lint": "./node_modules/.bin/eslint -c .eslintrc src bin config",
    "test": "mocha --colors --compilers js:babel-core/register --recursive ./src/**/*.test.js --ignore ./lib/**/*.test.js",
    "prepublish": "npm run compile"
  },
  "dependencies": {
    "autoprefixer": "~6.3",
    "babel-cli": "~6.5",
    "babel-core": "6.5",
    "babel-eslint": "~5.0.0-beta9",
    "babel-loader": "~6.2",
    "babel-plugin-react-transform": "~2.0",
    "babel-plugin-transform-decorators-legacy": "~1.3",
    "babel-plugin-transform-runtime": "~6.5",
    "babel-plugin-typecheck": "~3.6",
    "babel-preset-es2015": "~6.5",
    "babel-preset-react": "~6.5",
    "babel-preset-stage-0": "~6.5",
    "babel-runtime": "~6.5",
    "boom": "~3.1",
    "chai": "~3.5",
    "chai-immutable": "~1.5",
    "config": "~1.19",
    "css-loader": "~0.23",
    "eslint": "~1.10",
