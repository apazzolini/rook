    "redux-logger": "~2.5",
    "redux-thunk": "~1.0",
    "scroll-behavior": "~0.3",
    "serialize-javascript": "~1.1",
    "style-loader": "~0.13",
    "url-loader": "~0.5",
    "webpack": "~1.12",
    "webpack-config-merger": "~0.0",
